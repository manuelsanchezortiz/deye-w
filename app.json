{
  "configVersion": "v3",
  "app": {
    "appId": 22048,
    "appName": "Deye Watch",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.1"
    },
    "icon": "icon.png",
    "vender": "msneo",
    "description": "Show my Deye plant status"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.local_storage"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.5.0",
      "target": "3.5.0",
      "minVersion": "3.5"
    }
  },
  "targets": {
    "common": {
      "module": {
        "page": {
          "pages": [
            "pages/main/index",
            "pages/consumption/index"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "setting": {
          "path": "setting/index"
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 390
        }
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Fetch API"
    }
  },
  "defaultLanguage": "en-US"
}
